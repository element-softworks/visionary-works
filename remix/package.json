		"remix-seo": "^0.1.0",
		"remix-validated-form": "^4.1.8",
		"swiper": "^8.1.3",
		"tiny-invariant": "^1.2.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.2.0",
		"@remix-run/dev": "^1.3.5",
		"@types/lodash": "^4.14.179",
		"@types/react": "^18.0.1",
		"@types/react-dom": "^18.0.0",
		"@types/swiper": "^6.0.0",
		"esbuild-plugin-alias": "^0.2.1",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
