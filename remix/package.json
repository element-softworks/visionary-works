    "@remix-run/react": "^1.3.2",
    "@remix-validated-form/with-yup": "^2.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "prettier": "^2.6.0",
