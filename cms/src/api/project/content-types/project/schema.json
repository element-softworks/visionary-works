        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "slug": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
